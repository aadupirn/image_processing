gaussKernal(5, 5);
test = double(imread('testpattern.tif'));
[l, w] = size(test);
filter = gaussKernal(w, );
imshow(imfilter(test, filter, 'same'));

function w = gaussKernal(m, sigma)
    mat = zeros(m, m);
    k = 1/(2*pi*sigma^2);
    for i = 1 : m
        for j = 1 : m
            mat(i, j) = k*exp(-(i^2 + j^2)/(2*sigma^2));
        end
    end
    w = mat;
end